<!--pages/New/Online/index.wxml-->
<view wx:for='{{pagesData}}' wx:key='{{index}}' class='flex between aitems-center border-b-1 outer bg-fff'>
  <image class='imga' src='{{item.image}}'></image>
  <view class='flex1'>
    <view class='flex between'>
      <view class='fon-26 flex aitems-center bold'>{{item.userName}}
        <view wx:if='{{item.status}}' class='din'></view>
      </view>
      <view class='fon-24 c8'>{{item.time}}</view>
    </view>
  </view>
</view>