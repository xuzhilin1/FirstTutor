/* pages/New/qualification/index.wxss */

.top {
  width: 100%;
  height: 346rpx;
  padding: 30rpx;
}

.ZiPian {
  width: 100rpx;
  height: 100rpx;
}

.img {
  width: 100rpx;
  height: 100rpx;
  position: relative;
  top: 0;
  left: 0;
  overflow: hidden;
  box-sizing: border-box;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
}

.img-src {
  width: 100%;
  height: 100%;
}

.img-box {
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  position: absolute;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.1);
  display: flex;
  justify-content: center;
  align-items: center;
}

.img-box image {
  width: 50rpx;
  height: 50rpx;
}
