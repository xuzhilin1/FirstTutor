/*  评分  */

.score {
  position: relative;
  display: flex;
}

.score-all {
  display: flex;
}

.score-score {
  display: flex;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 10;
}

.score-list {
  width: 26rpx;
  height: 26rpx;
  margin-left: 6rpx;
  display: flex;
}

.score-img {
  width: 100%;
  height: 100%;
}

.score-image {
  height: 100%;
  width: 50%;
}

.teacher-box {
  display: flex;
  justify-content: space-between;
}

.teacher-l {
