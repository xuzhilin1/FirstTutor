/* pages/Home/foreignTeacher/index.wxss */

page {
  background: #fff;
}

.search-box {
  width: 100%;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
  background: #fff;
}

.search {
  box-sizing: border-box;
  width: 100%;
  border-radius: 20px;
  height: 50rpx;
  background-color: #f7f7f7;
  display: flex;
  align-items: center;
  padding: 0 20rpx;
}

.search input {
  height: 100%;
  width: 100%;
}

.img-top {
  width: 30rpx;
  height: 30rpx;
  margin-right: 20rpx;
}

.xia {
  width: 24rpx;
}

.table-wrapper {
  box-sizing: border-box;
  padding: 20rpx 30rpx;
  border-bottom: 1px solid #e0e0e0;
  font-size: 26rpx;
  display: flex;
  align-items: center;
  -webkit-align-items: center;
  justify-content: space-between;
  background: #fff;
}

.table-item {
  box-sizing: border-box;
  width: 24%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.middle-m {
  padding-top: 30rpx;
  padding-bottom: 40rpx;
}

.AnNiu {
  padding: 13rpx 20rpx;
  text-align: center;
  margin: 14rpx 0;
  margin-left: 10rpx;
}

.image-box {
  display: flex;
  align-items: center;
}

.url-image {
  width: 50rpx;
  border-radius: 50%;
  margin-right: 10rpx;
}

.url-gender {
  width: 24rpx;
  border-radius: 50%;
  margin-right: 10rpx;
}
.image-t-l{
  margin-right: 6rpx;
}
.image-t-r{
  margin-left: 6rpx;
}